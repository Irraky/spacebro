{
  "name": "spacebro",
  "version": "0.4.8",
  "description": "Spacebro uses mdns/zeroconf/bonjour and socket.io to bind apps together!",
  "keywords": [
    "zeroconf",
    "mdns",
    "realtime",
    "socket.io"
  ],
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run bin",
    "bin": "./bin/spacebro",
    "test": "standard src/**/*.js",
    "build": "./node_modules/.bin/babel src -d dist"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "blessed": "^0.1.81",
    "commander": "2.9.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "nodemon": "^1.11.0",
    "socket.io": "^2.0.1",
    "socketio-wildcard": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "spacebro-client": "^0.3.4",
    "standard": "^10.0.2"
  },
  "engines": {
    "node": ">=4.4.4"
  },
  "contributors": [
    "emmanuelgeoffray <manuel@soixantecircuits.fr> (https://github.com/emmanuelgeoffray)",
    "gabrielstuff <gabriel@soixantecircuits.fr> (http://twitter.com/gabrielstuff)",
    "hugohil <hugo@soixantecircuits.fr> (https://github.com/hugohil)"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/spacebro/spacebro.git"
  },
  "bin": {
    "spacebro": "./bin/spacebro"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "author": "Soixante circuits",
  "license": "MIT"
}
