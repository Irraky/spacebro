{
  "name": "spacebro",
  "version": "0.3.7",
  "description": "Spacebro use Zeroconf and socket.io to bind apps together!",
  "keywords": [
    "zeroconf",
    "mdns",
    "realtime",
    "socket.io"
  ],
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run bin",
    "client": "node dev/send-events.js",
    "example": "node dev/example.js",
    "bin": "./bin/spacebro",
    "bin-verbose": "./bin/spacebro --config ./config/config.json",
    "test": "standard src/**/*.js",
    "build": "./node_modules/.bin/babel src -d dist"
  },
  "dependencies": {
    "babel-runtime": "6.11.6",
    "blessed": "^0.1.81",
    "cli-table": "0.3.1",
    "colors": "1.1.2",
    "commander": "2.9.0",
    "lodash": "4.16.5",
    "mdns": "^2.3.3",
    "socket.io": "1.5.1",
    "socketio-wildcard": "^0.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "spacebro-client": "0.0.11",
    "standard": "^5.4.1"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "contributors": [
    "emmanuelgeoffray <manuel@soixantecircuits.fr> (https://github.com/emmanuelgeoffray)",
    "gabrielstuff <gabriel@soixantecircuits.fr> (http://twitter.com/gabrielstuff)",
    "hugohil <hugo@soixantecircuits.fr> (https://github.com/hugohil)"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/soixantecircuits/spacebro.git"
  },
  "bin": {
    "spacebro": "./bin/spacebro"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "author": "Soixante circuits",
  "license": "MIT"
}
